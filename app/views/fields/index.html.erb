<p id="notice"><%= notice %></p>

<h1><%= @fields.count %> champs</h1>
<%= link_to 'Nouveau champ', new_field_path %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Label</th>
    </tr>
  </thead>

  <tbody>
    <% @fields.each do |field| %>
      <tr>
        <td><%= link_to field, field %></td>
      </tr>
    <% end %>
  </tbody>
</table>