<p id="notice"><%= notice %></p>

<h1><%= @objectives.count %> objectives</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Module</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objectives.each do |objective| %>
      <tr>
        <td><%= objective %></td>
        <td><%= objective.teaching_module %></td>
        <td><%= link_to 'Show', objective %></td>
        <td><%= link_to 'Edit', edit_objective_path(objective) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
